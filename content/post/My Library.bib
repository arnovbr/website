}

@article{pettengell2008,
  title = {The Impact of Follicular Lymphoma on Health-Related Quality of Life},
  author = {Pettengell, R. and Donatti, C. and Hoskin, P. and Poynton, C. and Kettle, P. J. and Hancock, B. and Johnson, S. and Dyer, M. J. S. and Rule, S. and Walker, M. and Wild, D.},
