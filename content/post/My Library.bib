  author = {{Lobetti-Bodoni}, Chiara and Mantoan, Barbara and Monitillo, Luigia and Genuardi, Elisa and Drandi, Daniela and Barbero, Daniela and Bernocco, Elisa and Boccadoro, Mario and Ladetto, Marco},
